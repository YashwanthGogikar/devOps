pipeline {
  agent none
   stages {
    stage('Back-end'){
      agent { Docker {image 'maven:3.8.1-adoptopenjdk-11'}
    }
       
      steps{
        sh " mvm --version "
      }
    }
   stage('Front-end'){
      agent {
        Docker {image 'node:16-alpine'}
      }
      steps{
        sh "Node --version "
      }
    }
  }
}
  
