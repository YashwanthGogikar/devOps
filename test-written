pipeline {
  agent none
   stages {
    stage('Back-end'){
      agent {
        Docker {image 'maven:openjdk-jre-11'}
      }
      steps{
        sh " java --version "
      }
    }
   stage('Front-end'){
      agent {
        Docker {image 'nodejs:v24.8.0'}
      }
      steps{
        sh "Node --version "
      }
    }
  }
}
  
