pipeline {
  agent none
   stages {
    stage('Back-end'){
      agent {
        Docker {image 'maven:3.8.1-adoptopenjdk-11'}
      }
      steps{
        sh " java --version "
      }
    }
   }
    stage('Front-end'){
      agent {
        Docker {image 'nodejs:v24.8.0'}
      }
      steps{
        sh "Node --version "
      }
    }
   }
